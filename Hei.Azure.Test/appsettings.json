{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AppConfig": "<your app connection string>"
  },
  "AllowedHosts": "*",
  "AzureStorageConfig": {
    "ConnectionString": "< ConnectionString >",
    "ContainerName": "< ContainerName >",
    "TokenExpirationMinutes": 30
  },
  "FeatureManagement": {
    //简单功能开关
    "Beta": true,
    "v1": false,
    "v2": true,
    //百分率功能开关
    "PercentageFlag": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 30
          }
        }
      ]
    },
    //时间窗口功能开关
    "TimeWindowFlag": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "2022/08/03 08:04:00 +00:00", //这里是UTC时间
            "End": "2022/08/03 09:04:05 +00:00"
          }
        }
      ]
    },
    //自定义功能开关
    "CustomFeatureFlag": {
      "EnabledFor": [
        {
          "Name": "CustomFeature",
          "Parameters": {
            "AllowedPlatforms": [ //这里配置运行启用功能的平台
              "phone",
              "pad"
              //"pc"
            ]
          }
        }
      ]
    }
  }
}